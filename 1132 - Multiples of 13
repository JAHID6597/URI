#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,i,p,q,s=0;
    cin>>a>>b;
    if(a>b)
    {
        p=a;
        q=b;
    }
    if(a<b)
    {
        p=b;
        q=a;
    }
    for(i=q; i<=p; i++)
    {
        if(i%13!=0)
        {
            s=s+i;
        }
        else
        {
            s=s;
        }

    }
    cout<<s<<endl;
    return 0;
}
